<?xml version='1.1' encoding='UTF-8'?>
<jenkins.plugins.publish__over__ftp.BapFtpPublisherPlugin_-Descriptor plugin="publish-over-ftp@1.15">
  <hostConfigurations>
    <jenkins.plugins.publish__over__ftp.BapFtpHostConfiguration>
      <name>{{ prefix }}-exchange</name>
      <hostname>{{ hostvars.ftp.ansible_host }}</hostname>
      <username>exchange</username>
      <secretPassword>{{ hostvars.ftp.ftp_exchange_password }}</secretPassword>
      <remoteRootDir>/public/dists</remoteRootDir>
      <port>21</port>
      <timeout>300000</timeout>
      <useActiveData>false</useActiveData>
      <disableMakeNestedDirs>false</disableMakeNestedDirs>
      <disableRemoteVerification>false</disableRemoteVerification>
      <useFtpOverTls>false</useFtpOverTls>
      <useImplicitTls>false</useImplicitTls>
    </jenkins.plugins.publish__over__ftp.BapFtpHostConfiguration>
  </hostConfigurations>
  <defaults class="jenkins.plugins.publish_over_ftp.options.FtpPluginDefaults"/>
</jenkins.plugins.publish__over__ftp.BapFtpPublisherPlugin_-Descriptor>